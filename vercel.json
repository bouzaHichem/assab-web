{
  "functions": {
    "src/app/api/**/*.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "1",
      "PRISMA_GENERATE_SKIP_POSTINSTALL_FOR_VERCEL": "1"
    }
  },
  "buildCommand": "next build",
  "installCommand": "npm install && npx prisma generate",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- src/ next.config.js package.json"
}
